<h1>Search result</h1>

<h2>{{searchTerm}}</h2>
<main>
  <ul>
    {{#each shows}}
    <li><a href="/shows/{{this.show.id}}">{{this.show.name}}</a></li>
    {{/each}}
  </ul>
</main>

{{#if hasErrors}}  <!-- If no matches are found, you will print the following HTML paragraph -->
  <p class="not-found">{{error}}</p>
{{/if}}

<a href="/">Make another search</a>  <!--  links back to "/" route -->






